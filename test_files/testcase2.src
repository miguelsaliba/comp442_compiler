function add(x: int, y: int) => int {   // 2.2
    return(x + y);
}

function main() => void {
    local val: int;
    local input: int;

    val := 5;
    read(input);                        // 3.4

    local result: int;
    if (input > val) then {             // 3.2
        result := val;
    } else {
        result := input;
    };

    write(add(result, 10));             // 3.4
}